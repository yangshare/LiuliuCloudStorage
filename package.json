{
  "name": "liuliu-cloud-storage",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "description": "溜溜网盘 - 基于 Alist 的私有云盘客户端",
  "main": "dist-electron/main/index.js",
  "scripts": {
